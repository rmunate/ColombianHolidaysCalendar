{
    "name": "rmunate/calendario-colombia",
    "description": "This package simplifies working with Colombian holidays. It provides an easy, reliable, and convenient way to query, manipulate, or retrieve data related to holidays in Colombia, including detailed information about the reasons for each holiday.",
    "keywords": [
        "colombia",
        "calendar",
        "holidays",
        "festivos",
        "feriados",
        "rmunate"
    ],
    "homepage": "https://github.com/rmunate/ColombianCalendar",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rmunate\\Calendar\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Raul Mauricio UÃ±ate Castro",
            "email": "raulmauriciounate@gmail.com",
            "homepage": "https://github.com/rmunate",
            "role": "owner"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-sqlite3": "*",
        "ext-pdo_sqlite": "*",
        "nesbot/carbon": "^2.72.2|^3.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4|^11.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "v2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
